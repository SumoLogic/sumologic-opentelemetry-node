    "sumologic",
    "sumo",
    "node",
    "ot"
  ],
  "devDependencies": {
    "@types/node": "^17.0.9",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.0.4",
    "@opentelemetry/auto-instrumentations-node": "^0.27.1",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.27.0",
    "@opentelemetry/sdk-node": "^0.27.0"
